; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[android]

modules="org/godotengine/godot/GodotAdMob"

[application]

config/name="Avengers: Jewelry Defense"
run/main_scene="res://scenes/game/game_splashScreen.tscn"
boot_splash/image="res://sprites/gui/bg.png"
config/icon="res://icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[autoload]

game="*res://scripts/global/game.gd"
scene="*res://scripts/global/scene.gd"
admob="*res://scripts/global/admob.gd"
save_load="*res://scripts/global/save_load.gd"
object_musicPlayer="*res://scenes/objects/object_musicPlayer.tscn"
object_audioPlayer="*res://scenes/objects/object_audioPlayer.tscn"

[display]

window/size/width=420
window/size/height=720
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
